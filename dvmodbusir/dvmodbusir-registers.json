                    "size": 1,
                    "type": "uint16",
                    "registerType": "holdingRegister",
                    "description": "Serialnumber of read-out head.",
                    "defaultValue": 0,
                    "access": "RO"
                },
                {
                    "id": "dummy0",
                    "address": 1,
                    "size": 3,
                    "type": "raw",
                    "registerType": "holdingRegister",
                    "description": "Dummy0",
                    "access": "RO"
                },
                {
                    "id": "deviceStatus",
                    "address": 4,
                    "size": 1,
                    "type": "uint16",
                    "registerType": "holdingRegister",
                    "description": "Info about connected meters.",
                    "defaultValue": 0,
                    "access": "RO"
                }
            ]
        },
        {
            "id": "modbusInfo",
            "readSchedule": "init",
            "registers": [
                {
                    "id": "meterData",
